{
  "name": "react-tdd",
  "version": "1.0.0",
  "description": "test driven development with react.js",
  "engines": {
    "node": "4.4.2"
  },
  "main": "index.js",
  "scripts": {
    "version": "node version-check.js",
    "lint": "eslint . --ext .js --fix",
    "test": "jasmine",
    "build": "echo \"BUILD OK\" && exit 0",
    "start": "http-server src",
    "prebuild": "npm run version && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bohandley/react-tdd.git"
  },
  "keywords": [
    "react",
    "node",
    "tdd",
    "npm"
  ],
  "author": "Brendan O'Handley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bohandley/react-tdd/issues"
  },
  "homepage": "https://github.com/bohandley/react-tdd#readme",
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-plugin-react": "^7.5.1",
    "http-server": "^0.10.0",
    "jasmine": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-commonjs": "^1.0.0",
    "karma-jasmine": "^1.1.1"
  }
}
