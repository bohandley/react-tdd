{
  "name": "react-tdd",
  "version": "1.0.0",
  "description": "test driven development with react.js",
  "engines": {
    "node": "4.4.2"
  },
  "main": "index.js",
  "scripts": {
    "version": "node version-check.js",
    "lint": "eslint . --ext .js --fix",
    "jasmine": "jasmine",
    "test": "karma start --singleRun",
    "build": "echo \"BUILD OK\" && exit 0",
    "start": "http-server dist",
    "bundle": "webpack",
    "prebuild": "npm run version && npm run lint && npm run test && npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bohandley/react-tdd.git"
  },
  "keywords": [
    "react",
    "node",
    "tdd",
    "npm"
  ],
  "author": "Brendan O'Handley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bohandley/react-tdd/issues"
  },
  "homepage": "https://github.com/bohandley/react-tdd#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "eslint": "^4.14.0",
    "eslint-plugin-react": "^7.5.1",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "jasmine": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-commonjs": "^1.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-webpack": "^2.0.9",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
